  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "2.15.0",
    "@sap-cloud-sdk/resilience": "3.12.0",
    "@sap/cds": "^8.1.1",
    "@sap/cds-dk": "7.6.0",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1.7.3"
  },
  "scripts": {
    "start": "cds-serve",
    "start1": "cds deploy && cds watch",
    "start-sqlite": "cds deploy --to sqlite:db.sqlite && cds run"
  },
  "cds": {
    "requires": {
      "API_BUSINESS_PARTNER": {
        "kind": "odata-v2",
        "model": "srv/external/API_BUSINESS_PARTNER"
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db.sqlite"
        }
      }
    }
  }
