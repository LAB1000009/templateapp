<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.suite.ui.microchart" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:l="sap.ui.layout" xmlns:m="sap.m" height="100%" displayBlock="true">
   <m:Panel height="100%" headerText="{i18n>DashboardTab_Title}" id="dashboarPage" class="contentPadding" backgroundDesign="List">
      <m:VBox height="100%" width="100%">
         <m:Panel expanded="true" headerText="Example 1" expandable="true">
            <m:GenericTile header="Revenue Dynamics" frameType="OneByOne" press="press">
               <m:tileContent>
                  <m:TileContent footer="Column chart">
                     <m:content>
                        <ColumnMicroChart size="Responsive">
                           <columns>
                              <ColumnMicroChartData value="65" color="Error" />
                              <ColumnMicroChartData value="90" color="Error" />
                              <ColumnMicroChartData value="75" color="Error" />
                              <ColumnMicroChartData value="95" color="Error" />
                              <ColumnMicroChartData value="100" color="Error" />
                              <ColumnMicroChartData value="90" color="Error" />
                              <ColumnMicroChartData value="80" color="Error" />
                              <ColumnMicroChartData value="35" color="Neutral" />
                              <ColumnMicroChartData value="65" color="Neutral" />
                              <ColumnMicroChartData value="55" color="Neutral" />
                              <ColumnMicroChartData value="60" color="Neutral" />
                              <ColumnMicroChartData value="5" color="Neutral" />
                              <ColumnMicroChartData value="15" color="Neutral" />
                              <ColumnMicroChartData value="15" color="Neutral" />
                           </columns>
                        </ColumnMicroChart>
                     </m:content>
                  </m:TileContent>
               </m:tileContent>
            </m:GenericTile>
            <m:GenericTile header="Revenue Dynamics" frameType="TwoByOne" press="press" class="sapUiSmallMarginBegin">
               <m:tileContent>
                  <m:TileContent footer="Column chart with labels">
                     <m:content>
                        <ColumnMicroChart size="Responsive">
                           <leftTopLabel>
                              <ColumnMicroChartLabel label="0M" />
                           </leftTopLabel>
                           <rightTopLabel>
                              <ColumnMicroChartLabel label="80M" />
                           </rightTopLabel>
                           <leftBottomLabel>
                              <ColumnMicroChartLabel label="June 1" />
                           </leftBottomLabel>
                           <rightBottomLabel>
                              <ColumnMicroChartLabel label="June 30" />
                           </rightBottomLabel>
                           <columns>
                              <ColumnMicroChartData value="65" color="Error" />
                              <ColumnMicroChartData value="90" color="Error" />
                              <ColumnMicroChartData value="75" color="Error" />
                              <ColumnMicroChartData value="95" color="Error" />
                              <ColumnMicroChartData value="100" color="Error" />
                              <ColumnMicroChartData value="90" color="Error" />
                              <ColumnMicroChartData value="80" color="Error" />
                              <ColumnMicroChartData value="35" color="Neutral" />
                              <ColumnMicroChartData value="65" color="Neutral" />
                              <ColumnMicroChartData value="55" color="Neutral" />
                              <ColumnMicroChartData value="60" color="Neutral" />
                              <ColumnMicroChartData value="5" color="Neutral" />
                              <ColumnMicroChartData value="15" color="Neutral" />
                              <ColumnMicroChartData value="15" color="Neutral" />
                           </columns>
                        </ColumnMicroChart>
                     </m:content>
                  </m:TileContent>
               </m:tileContent>
               <m:tileContent>
                  <m:TileContent footer="Column chart">
                     <m:content>
                        <ColumnMicroChart size="Responsive">
                           <columns>
                              <ColumnMicroChartData value="60" color="Error" />
                              <ColumnMicroChartData value="100" color="Error" />
                              <ColumnMicroChartData value="-20" color="Error" />
                              <ColumnMicroChartData value="60" color="Neutral" />
                              <ColumnMicroChartData value="55" color="Neutral" />
                              <ColumnMicroChartData value="15" color="Neutral" />
                           </columns>
                        </ColumnMicroChart>
                     </m:content>
                  </m:TileContent>
               </m:tileContent>
            </m:GenericTile>
         </m:Panel>
         <m:Panel expanded="true" expandable="true" headerText="Example 2">
            <m:HBox>
               <ComparisonMicroChart size="S" scale="M" class="sapUiSmallMargin" press="press">
                  <data>
                     <ComparisonMicroChartData title="EMEA" displayValue="USD 0" color="Good" />
                     <ComparisonMicroChartData title="EMEA" value="50" color="Good" />
                     <ComparisonMicroChartData title="APAC" value="-20" color="Error" />
                  </data>
               </ComparisonMicroChart>
               <ComparisonMicroChart size="M" scale="M" class="sapUiSmallMargin" press="press">
                  <data>
                     <ComparisonMicroChartData title="Americas" value="10" color="Good" />
                     <ComparisonMicroChartData title="EMEA" value="50" color="Good" />
                     <ComparisonMicroChartData title="APAC" value="-20" color="Error" />
                  </data>
               </ComparisonMicroChart>
               <ComparisonMicroChart size="M" scale="M" class="sapUiSmallMargin" press="press" shrinkable="true" colorPalette="sapUiChartPaletteQualitativeHue1, sapUiChartPaletteQualitativeHue2, sapUiChartPaletteQualitativeHue3,                   sapUiChartPaletteQualitativeHue4, sapUiChartPaletteQualitativeHue5, sapUiChartPaletteQualitativeHue6, sapUiChartPaletteQualitativeHue7,                   sapUiChartPaletteQualitativeHue8, sapUiChartPaletteQualitativeHue9, sapUiChartPaletteQualitativeHue10, sapUiChartPaletteQualitativeHue11">
                  <data>
                     <ComparisonMicroChartData title="Americas" value="10" />
                     <ComparisonMicroChartData title="EMEA" value="50" />
                     <ComparisonMicroChartData title="APAC" value="-20" />
                     <ComparisonMicroChartData title="LTA" value="60" />
                     <ComparisonMicroChartData title="ALPS" value="-20" />
                     <ComparisonMicroChartData title="Australia" value="55" />
                  </data>
               </ComparisonMicroChart>
            </m:HBox>
         </m:Panel>
         <m:Panel headerText="Example 3" expanded="true" expandable="true">
            <m:HBox>
               <m:GenericTile header="Revenue Dynamics" frameType="TwoByOne" press="press">
                  <m:tileContent>
                     <m:TileContent footer="Red Delta">
                        <m:content>
                           <DeltaMicroChart value1="-105" size="Responsive" value2="-98" title1="May 2014" title2="Apr 2014" displayValue1="-105 Mio" displayValue2="-98 Mio" deltaDisplayValue="7 Mio" color="Error" />
                        </m:content>
                     </m:TileContent>
                     <m:TileContent footer="Yellow Delta">
                        <m:content>
                           <DeltaMicroChart value1="-7" size="Responsive" value2="16" title1="May 2014" title2="Apr 2014" displayValue1="-7 Mio" displayValue2="16 Mio" deltaDisplayValue="23 Mio" color="Critical" />
                        </m:content>
                     </m:TileContent>
                  </m:tileContent>
               </m:GenericTile>
               <m:GenericTile press="press" class="sapUiSmallMarginBegin" header="Total Income Change" subheader="By year" frameType="TwoByOne" state="Loaded">
                  <m:tileContent>
                     <m:TileContent unit="EUR" footer="Current vs previous" frameType="TwoByOne">
                        <m:content>
                           <DeltaMicroChart value1="35" value2="33" size="Responsive" title1="Jan 2015" title2="Jan 2014" displayValue1="35 Mio" displayValue2="33 Mio" deltaDisplayValue="2 Mio" color="Good" />
                        </m:content>
                     </m:TileContent>
                  </m:tileContent>
               </m:GenericTile>
               <m:GenericTile header="Revenue Dynamics" class="sapUiSmallMarginBegin" frameType="OneByOne" press="press">
                  <m:tileContent>
                     <m:TileContent footer="Green Delta">
                        <m:content>
                           <DeltaMicroChart value1="105" size="Responsive" value2="98" title1="May 2014" title2="Apr 2014" displayValue1="105 Mio" displayValue2="98 Mio" deltaDisplayValue="7 Mio" color="Good" />
                        </m:content>
                     </m:TileContent>
                  </m:tileContent>
               </m:GenericTile>
               <m:GenericTile header="Total Income Change" class="sapUiSmallMarginBegin" subheader="By year" frameType="OneByOne" press="press">
                  <m:tileContent>
                     <m:TileContent footer="Current vs previous" unit="EUR">
                        <m:content>
                           <DeltaMicroChart value1="35" value2="33" size="Responsive" title1="Jan 2015" title2="Jan 2014" displayValue1="35 Mio" displayValue2="33 Mio" deltaDisplayValue="2 Mio" color="Good" />
                        </m:content>
                     </m:TileContent>
                  </m:tileContent>
               </m:GenericTile>
            </m:HBox>
         </m:Panel>
         <m:Panel headerText="Example 4" expanded="true" expandable="true">
            <m:HBox alignItems="Center">
               <StackedBarMicroChart size="M" press="press" class="sapUiSmallMargin">
                  <bars>
                     <StackedBarMicroChartBar displayValue="20M" value="20" />
                     <StackedBarMicroChartBar displayValue="40M" value="40" />
                     <StackedBarMicroChartBar displayValue="50M" value="50" />
                  </bars>
               </StackedBarMicroChart>
               <StackedBarMicroChart size="L" press="press" precision="0" class="sapUiSmallMargin">
                  <bars>
                     <StackedBarMicroChartBar valueColor="Good" value="40" />
                     <StackedBarMicroChartBar valueColor="Critical" value="20" />
                     <StackedBarMicroChartBar valueColor="Error" value="50" />
                  </bars>
               </StackedBarMicroChart>
               <StackedBarMicroChart size="S" press="press" precision="0" class="sapUiSmallMargin">
                  <bars>
                     <StackedBarMicroChartBar valueColor="sapUiChartPaletteSemanticGood" value="20" />
                     <StackedBarMicroChartBar valueColor="sapUiChartPaletteSemanticNeutral" value="40" />
                     <StackedBarMicroChartBar valueColor="sapUiChartPaletteSemanticCritical" value="50" />
                  </bars>
               </StackedBarMicroChart>
               <StackedBarMicroChart size="XS" press="press" precision="0" class="sapUiSmallMargin">
                  <bars>
                     <StackedBarMicroChartBar displayValue="20M" value="20" />
                     <StackedBarMicroChartBar displayValue="40M" valueColor="Neutral" value="40" />
                     <StackedBarMicroChartBar displayValue="50M" valueColor="sapUiChartPaletteSemanticBad" value="50" />
                  </bars>
               </StackedBarMicroChart>
               <StackedBarMicroChart size="L" press="press" precision="0" class="sapUiSmallMargin" />
            </m:HBox>
         </m:Panel>
         <m:Panel headerText="Example 5" expanded="true" expandable="true">
            <m:VBox alignItems="Center" renderType="Bare">
               <m:Panel headerText="Multiple lines">
                  <l:Grid defaultSpan="XL6 L6 M6 S12" content="{dashboardModel>/multiline}">
                     <l:VerticalLayout class="sapUiTinyMargin">
                        <m:Text text="{dashboardModel>description}" width="17rem" />
                        <LineMicroChart color="{dashboardModel>color}" showPoints="{dashboardModel>showPoints}" size="{dashboardModel>/size}" threshold="{dashboardModel>threshold}" showThresholdLine="{dashboardModel>showThresholdLine}" leftTopLabel="{dashboardModel>leftTopLabel}" leftBottomLabel="{dashboardModel>leftBottomLabel}" rightTopLabel="{dashboardModel>rightTopLabel}" rightBottomLabel="{dashboardModel>rightBottomLabel}" showTopLabels="{dashboardModel>showTopLabels}" showBottomLabels="{dashboardModel>showBottomLabels}" press="press" class="sapUiTinyMarginTop" lines="{                             path: 'dashboardModel>lines',                             templateShareable: true                         }">
                           <lines>
                              <LineMicroChartLine points="{                                     path: 'dashboardModel>points',                                     templateShareable: true                                 }" color="{dashboardModel>color}" showPoints="{dashboardModel>showPoints}" type="{dashboardModel>type}">
                                 <LineMicroChartPoint x="{dashboardModel>x}" y="{dashboardModel>y}" />
                              </LineMicroChartLine>
                           </lines>
                        </LineMicroChart>
                     </l:VerticalLayout>
                  </l:Grid>
               </m:Panel>
               <m:Panel headerText="Regular Points">
                  <l:Grid defaultSpan="XL6 L6 M6 S12" content="{dashboardModel>/regular}">
                     <l:VerticalLayout class="sapUiTinyMargin">
                        <m:Text text="{dashboardModel>description}" width="17rem" />
                        <LineMicroChart showPoints="{dashboardModel>showPoints}" size="{dashboardModel>/size}" threshold="{dashboardModel>threshold}" leftTopLabel="{dashboardModel>leftTopLabel}" leftBottomLabel="{dashboardModel>leftBottomLabel}" rightTopLabel="{dashboardModel>rightTopLabel}" rightBottomLabel="{dashboardModel>rightBottomLabel}" color="{dashboardModel>color}" press="press" lines="{                             path: 'dashboardModel>/lines',                             templateShareable: true                         }" showThresholdValue="{dashboardModel>showThresholdValue}" thresholdDisplayValue="{dashboardModel>thresholdDisplayValue}" class="sapUiTinyMarginTop">
                           <lines>
                              <LineMicroChartLine points="{                                     path: 'dashboardModel>points',                                     templateShareable: true                                 }">
                                 <LineMicroChartPoint x="{dashboardModel>x}" y="{dashboardModel>y}" />
                              </LineMicroChartLine>
                           </lines>
                        </LineMicroChart>
                     </l:VerticalLayout>
                  </l:Grid>
               </m:Panel>
               <m:Panel headerText="Emphasized Points">
                  <l:Grid defaultSpan="XL6 L6 M6 S12" content="{dashboardModel>/emphasized}">
                     <l:VerticalLayout class="sapUiTinyMargin">
                        <m:Text text="{dashboardModel>description}" width="17rem" />
                        <LineMicroChart size="{dashboardModel>/size}" threshold="{dashboardModel>threshold}" leftTopLabel="{dashboardModel>leftTopLabel}" leftBottomLabel="{dashboardModel>leftBottomLabel}" rightTopLabel="{dashboardModel>rightTopLabel}" rightBottomLabel="{dashboardModel>rightBottomLabel}" color="{dashboardModel>color}" press="press" lines="{                             path: 'dashboardModel>lines',                             templateShareable: true                         }" class="sapUiTinyMarginTop">
                           <lines>
                              <LineMicroChartLine points="{                                     path: 'dashboardModel>points',                                     templateShareable: true                                 }">
                                 <LineMicroChartEmphasizedPoint x="{dashboardModel>x}" y="{dashboardModel>y}" color="{dashboardModel>color}" show="{dashboardModel>show}" />
                              </LineMicroChartLine>
                           </lines>
                        </LineMicroChart>
                     </l:VerticalLayout>
                  </l:Grid>
               </m:Panel>
               <m:Panel headerText="No Points">
                  <LineMicroChart size="L" leftTopLabel="120 Mio" leftBottomLabel="140 Mio" rightTopLabel="Sept 2016" rightBottomLabel="Oct 2016" press="press" class="sapUiTinyMarginTop" />
               </m:Panel>
            </m:VBox>
         </m:Panel>
      </m:VBox>
   </m:Panel>
</core:FragmentDefinition>