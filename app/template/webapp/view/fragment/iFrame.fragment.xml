<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" >
   <Dialog stretch="true" title="{i18n>iframe_dialog_Title}" draggable="true" resizable="true">
      <Page class="sapUiContentPadding" showHeader="false">
         <content>
          <core:HTML content="&lt;iframe src='https://sapui5.hana.ondemand.com/sdk' width='100%' height='95%'&gt;&lt;/iframe&gt;"/>
         </content>
      </Page>
      <endButton>
         <Button text="Close" press="onCloseFooterDialog" />
      </endButton>
   </Dialog>
</core:FragmentDefinition>