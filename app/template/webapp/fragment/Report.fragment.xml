                           </f:FormElement>
                        </f:formElements>
                     </f:FormContainer>
                  </f:formContainers>
               </f:Form>
            </Panel>
         </content>
      </Page>
      <endButton>
         <Button text="Close" press="onCloseFooterDialog" />
      </endButton>
   </Dialog>
</core:FragmentDefinition>