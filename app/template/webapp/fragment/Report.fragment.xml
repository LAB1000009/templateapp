
<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form">
   <Dialog  title="Dashboard details" draggable="true" resizable="true" contentWidth="500px" contentHeight="1000px">
      <Page class="sapUiContentPadding" showHeader="false">
         <content>
           
               <Panel class="">
                  <f:Form editable="false">
                
                     <f:layout>
                        <f:ResponsiveGridLayout labelSpanXL="12" labelSpanL="12" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="2" columnsL="5" columnsM="1" singleContainerFullSize="false" />
                     </f:layout>
                     <f:title>
                        <core:Title text="Report Information" />
                     </f:title>
                     <f:formContainers>
                        <!-- Report Information Section -->
                        <f:FormContainer>
                           <f:formElements>
                              <f:FormElement label="Owner">
                                 <f:fields>
                                    <Text text="Steve Otieno" />
                                 </f:fields>
                              </f:FormElement>
                           </f:formElements>
                        </f:FormContainer>
                        <f:FormContainer>
                           <f:formElements>
                              <f:FormElement label="Objective">
                                 <f:fields>
                                    <Text text="Measure staff turnover" />
                                 </f:fields>
                              </f:FormElement>
                           </f:formElements>
                        </f:FormContainer>
                        <f:FormContainer>
                           <f:formElements>
                              <f:FormElement label="Date of Creation">
                                 <f:fields>
                                    <Text text="14.05.2022" />
                                 </f:fields>
                              </f:FormElement>
                           </f:formElements>
                        </f:FormContainer>
                        <f:FormContainer>
                           <f:formElements>
                              <f:FormElement label="Version">
                                 <f:fields>
                                    <Text text="1.0" />
                                 </f:fields>
                              </f:FormElement>
                           </f:formElements>
                        </f:FormContainer>
                        <f:FormContainer>
                           <f:formElements>
                              <f:FormElement label="Report Designer">
                                 <f:fields>
                                    <Text text="Steve Otieno" />
                                 </f:fields>
                              </f:FormElement>
                           </f:formElements>
                        </f:FormContainer>
                     </f:formContainers>
                  </f:Form>
               </Panel>
                <Panel class="sapUiSmallMarginTop">
                  <f:Form editable="false">
                     <f:layoutData>
                           <FlexItemData
                              growFactor="1"
                              baseSize="0"
                              backgroundDesign="Solid"
                              styleClass="sapUiTinyMargin" />
                     </f:layoutData>
                     <f:layout>
                           <f:ResponsiveGridLayout
                              labelSpanXL="12"
                              labelSpanL="12"
                              labelSpanM="12"
                              labelSpanS="12"
                              adjustLabelSpan="false"
                              emptySpanXL="4"
                              emptySpanL="4"
                              emptySpanM="4"
                              emptySpanS="0"
                              columnsXL="2"
                              columnsL="2"
                              columnsM="1"
                              singleContainerFullSize="false" />
                     </f:layout>
                     <f:title>
                           <core:Title text="Report Information" />
                     </f:title>
                     <f:formContainers>
                           <f:FormContainer>
                              <f:formElements>
                                 <f:FormElement label="Report Name [Technical Name]">
                                       <f:fields>
                                          <Text text="Empty" />
                                       </f:fields>
                                 </f:FormElement>
                              </f:formElements>
                           </f:FormContainer>

                           <f:FormContainer>
                              <f:formElements>
                                 <f:FormElement label="Classification and Cluster">
                                       <f:fields>
                                          <Text text="Empty" />
                                       </f:fields>
                                 </f:FormElement>
                              </f:formElements>
                           </f:FormContainer>
                     </f:formContainers>
                  </f:Form>
               </Panel>

              <Panel class="sapUiSmallMarginTop">
    <f:Form editable="false">
        <f:layout>
            <f:ResponsiveGridLayout
                labelSpanXL="12"
                labelSpanL="12"
                labelSpanM="12"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="4"
                emptySpanL="4"
                emptySpanM="4"
                emptySpanS="0"
                columnsXL="6"
                columnsL="6"
                columnsM="1"
                singleContainerFullSize="false" />
        </f:layout>
        <f:title>
            <core:Title text="Report Details" />
        </f:title>
        <f:formContainers>
            <f:FormContainer>
                <f:formElements>
                    <f:FormElement >
                    <f:label>
                     <Label design="Bold" text='Goal (Problem solved by report)'/>
                    </f:label>
                        <f:fields>
                            <Text text="Empty" />
                        </f:fields>
                    </f:FormElement>
                </f:formElements>
            </f:FormContainer>

            <f:FormContainer>
                <f:formElements>
                    <f:FormElement label="Process">
                        <f:fields>
                            <Text text="Empty" />
                        </f:fields>
                    </f:FormElement>
                </f:formElements>
            </f:FormContainer>

            <f:FormContainer>
                <f:formElements>
                    <f:FormElement label="Time to Target">
                        <f:fields>
                            <Text text="Empty" />
                        </f:fields>
                    </f:FormElement>
                </f:formElements>
            </f:FormContainer>

            <f:FormContainer>
                <f:formElements>
                    <f:FormElement label="Target Audience">
                        <f:fields>
                            <Text text="Empty" />
                        </f:fields>
                    </f:FormElement>
                </f:formElements>
            </f:FormContainer>

            <f:FormContainer>
                <f:formElements>
                    <f:FormElement label="Necessary Resources">
                        <f:fields>
                            <Text text="Empty" />
                        </f:fields>
                    </f:FormElement>
                </f:formElements>
            </f:FormContainer>

            <f:FormContainer>
                <f:formElements>
                    <f:FormElement label="Known Issues and Troubleshooting">
                        <f:fields>
                            <Text text="Empty" />
                        </f:fields>
                    </f:FormElement>
                </f:formElements>
            </f:FormContainer>
        </f:formContainers>
    </f:Form>
</Panel>

               <Panel class="sapUiSmallMarginTop">
                  <f:Form editable="false">
                     <f:layout>
                        <f:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="1" singleContainerFullSize="false" />
                     </f:layout>
                     <f:title>
                        <core:Title text="Data Source" />
                     </f:title>
                     <f:formContainers>
                        <f:FormContainer>
                           <f:formElements>
                              <f:FormElement label="Data Source">
                                 <f:fields>
                                    <Text text="DWH, Ext. DB, Flatfiles" />
                                 </f:fields>
                              </f:FormElement>
                              <f:FormElement label="Data Quality Assurance">
                                 <f:fields>
                                    <Text text="Performed, Not Performed" />
                                 </f:fields>
                              </f:FormElement>
                              <f:FormElement label="Schedules and Automated Refresh">
                                 <f:fields>
                                    <Text text="Automated Refreshes" />
                                 </f:fields>
                              </f:FormElement>
                              <f:FormElement label="Schedule Responsible">
                                 <f:fields>
                                    <Text text="Analyst | Expert" />
                                 </f:fields>
                              </f:FormElement>
                           </f:formElements>
                        </f:FormContainer>
                     </f:formContainers>
                  </f:Form>
               </Panel>
            
            <Panel class="sapUiSmallMarginTop">
               <f:Form editable="false">
                  <f:layout>
                     <f:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="1" singleContainerFullSize="false" />
                  </f:layout>
                  <f:title>
                     <core:Title text="ChangeLog Intake" />
                  </f:title>
                  <f:formContainers>
                     <!-- ChangeLog Intake Section -->
                     <f:FormContainer>
                        <f:formElements>
                           <f:FormElement label="Date">
                              <f:fields>
                                 <Text text="Empty" />
                              </f:fields>
                           </f:FormElement>
                           <f:FormElement label="Requested By">
                              <f:fields>
                                 <Text text="Empty" />
                              </f:fields>
                           </f:FormElement>
                           <f:FormElement label="Description">
                              <f:fields>
                                 <Text text="Empty" />
                              </f:fields>
                           </f:FormElement>
                        </f:formElements>
                     </f:FormContainer>
                  </f:formContainers>
               </f:Form>
            </Panel>
            <Panel class="sapUiSmallMarginTop">
               <f:Form editable="false">
                  <f:layout>
                     <f:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="1" singleContainerFullSize="false" />
                  </f:layout>
                  <f:title>
                     <core:Title text="KPI and Measures" />
                  </f:title>
                  <f:formContainers>
                     <!-- KPI and Measures Section -->
                     <f:FormContainer>
                        <f:formElements>
                           <f:FormElement label="KPI">
                              <f:fields>
                                 <Text text="Empty" />
                              </f:fields>
                           </f:FormElement>
                           <f:FormElement label="Measures">
                              <f:fields>
                                 <Text text="Empty" />
                              </f:fields>
                           </f:FormElement>
                        </f:formElements>
                     </f:FormContainer>
                  </f:formContainers>
               </f:Form>
            </Panel>
            <Panel class="sapUiSmallMarginTop">
               <f:Form editable="false">
                  <f:layout>
                     <f:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="1" singleContainerFullSize="false" />
                  </f:layout>
                  <f:title>
                     <core:Title text="Project Documentation" />
                  </f:title>
                  <f:formContainers>
                     <f:FormContainer>
                        <f:formElements>
                           <f:FormElement label="Roadmap">
                              <f:fields>
                                 <Text text="Empty" />
                              </f:fields>
                           </f:FormElement>
                        </f:formElements>
                     </f:FormContainer>
                     <f:FormContainer>
                        <f:formElements>
                           <f:FormElement label="Related Report">
                              <f:fields>
